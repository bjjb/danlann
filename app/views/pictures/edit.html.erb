<% title "Edit Picture" %>
<% form_for @picture, :html => { :multipart => true } do |f| %>
  <%= f.text_field :name %>
  <%= f.text_area :description %>
  <%= f.text_field :tag_names %>
  <%= f.text_field :rotation %>
  <%= f.submit "Save" %>
  <%= link_to 'Cancel', picture_path(@picture) %>
<% end %>


